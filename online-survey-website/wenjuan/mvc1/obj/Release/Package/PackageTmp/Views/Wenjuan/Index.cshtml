			break;
		case 24:
			document.getElementById("optionstitle").style.opacity=
			document.getElementById("numbertd").style.opacity=
			document.getElementById("questiontd").style.opacity=0;
			document.getElementById("optionstitle").style.top=
			document.getElementById("numbertd").style.top=
			document.getElementById("questiontd").style.top="-1em";
			document.getElementById("o24").style.opacity=0;

			document.getElementById("o24").style.top="-1em";
			setTimeout(function(){
				callback();
			},550);
			break;

	    default:
	        setTimeout(function(){
	            callback();
	        },0);
	        break;
	}
}

function prevquestion()
{
	if(_state==1) return;
	if(_i==1) return;
	$("#button3").text("下一题");
	showquestion(_i-1);
}

function nextquestion()
{
	if(_state==1) return;
	if((_i==_n-1) || (_i==_n)) $("#button3").text("提交");
	else $("#button3").text("下一题");
	if(__debug) alert("next");
	if(_i==_n) {
	    submitanswers(function () {
	        document.getElementById("maindiv").style.top = "-1em";
	        document.getElementById("maindiv").style.opacity = 0;
	        cleardata();
	        setTimeout(function () {
	            $("#maindiv").load("/Content/Wenjuan/questions.html #epilogue", function () {
	                document.getElementById("epilogue").offsetHeight;
	                divresize2();
	                document.getElementById("maindiv").style.top = "0px";
	                document.getElementById("maindiv").style.opacity = 1;
	            });
	        }, 650);
	    });

		return;
	}
	showquestion(_i+1);
}

function submitanswers(callback)
{
	var j=0;
	var nullanswers=new Array();
	for(var i=1;i<=_n;i++) {
		if((_a[i]==0) && (_mul[i]==0)) {
			nullanswers[j]=i;
			j++;
		}
	}
	if(j>0){ //j>0
		alert("您第" + nullanswers.toString() + "题还没有做呢！");
		//return true;   //debug
		showquestion(nullanswers[0]);
		return false;
	}
	else {
		//alert("post");

	    $.post("/Wenjuan/Submit", { token: _token, a: _a.toString(), a_addi: _a_addi.toString() }, function (data) {
	        //alert("p");
	        if (data == "0")
	            callback();
		});

		
		
	}

}





function storedata()
{
	setCookie("wenjuan_a",_a.toString(),1);
	setCookie("wenjuan_c",_i,1);
	setCookie("wenjuan_a_addi", _a_addi.toString(), 1);
	setCookie("wenjuan_token", _token, 1);
}

function retrievedata()
{
	var s=getCookie("wenjuan_a");
	if(s=="") {
		_new=true
		return;
	}
	else _a=s.split(",");
	if(__debug) alert(_a.toString());
	s=getCookie("wenjuan_c");
	if(s=="") {
		_new=true
		return;
	}
	else _i=parseInt(s);
	if(__debug) alert(_i);
	s=getCookie("wenjuan_a_addi");
	if(s=="") {
		_new=true
		return;
	}
	else _a_addi = s.split(",");
	s = getCookie("wenjuan_token");
	if (s == "") {
	    _new = true
	    return;
	}
	else _token = s;
	if(__debug) alert(_a_addi.toString());
	
}

function cleardata()
{
	setCookie("wenjuan_a","",1);
	setCookie("wenjuan_c","",1);
	setCookie("wenjuan_a_addi","",1);
}



function getCookie(c_name){
　　　　if (document.cookie.length>0){　　
　　　　　　c_start=document.cookie.indexOf(c_name + "=");
　　　　　　if (c_start!=-1){ 
　　　　　　　　c_start=c_start + c_name.length+1;
　　　　　　　　c_end=document.cookie.indexOf(";",c_start);　　
　　　　　　　　if (c_end==-1) c_end=document.cookie.length;　　
　　　　　　　　return unescape(document.cookie.substring(c_start,c_end));　　
　　　　　　} 
　　　　}
　　　　return "";
}　

function setCookie(c_name, value, expiredays){
 　　　　var exdate=new Date();
 　　　　exdate.setDate(exdate.getDate() + expiredays);
 　　　　document.cookie=c_name+ "=" + escape(value) + ((expiredays==null) ? "" : ";expires="+exdate.toGMTString());
}

</script>



