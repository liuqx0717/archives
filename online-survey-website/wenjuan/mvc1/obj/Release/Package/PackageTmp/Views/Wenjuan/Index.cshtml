<script>

var _o24_r = new Array(25+1);
var _o24_current;
var _d;
var _o24_enabled=false;

function o24_rand(i)
{
	var r;
	while (!(r = Math.random()));
	return Math.ceil(Math.random() * i);
}

function o24_genrand(j)
{
	if(j==0) {
		var randtmp = new Array(25+1);
		for (var i = 0; i <= 25; i++)
			_o24_r[i]=randtmp[i] = i;
		
		for (var i = 25; i >= 1; i--) {
			var r = o24_rand(i);
			_o24_r[i] = randtmp[r];
			randtmp.splice(r, 1);
		}
		
	}
	else {
		var randtmp = new Array(25+1);
		for (var i = 0; i <= 25; i++)
			_o24_r[i]=randtmp[i] = i;
		_o24_r[j]=1;
		randtmp.splice(1, 1);
		for (var i = 25; i > j; i--) {
			var r = o24_rand(i-1);
			_o24_r[i] = randtmp[r];
			randtmp.splice(r, 1);
		}
		for (var i = j-1; i >= 1; i--) {
			var r = o24_rand(i);
			_o24_r[i] = randtmp[r];
			randtmp.splice(r, 1);
		}
	}
	_o24_current=1;
}

function o24_showrand(i)
{
	if(i==0) {
		o24_genrand(i);
		for(var i=1;i<=25;i++){
			if(_o24_r[i]<10)
				document.getElementById("o24_custom_"+i).innerHTML="0"+_o24_r[i];
			else
				document.getElementById("o24_custom_"+i).innerHTML=+_o24_r[i];
			document.getElementById("o24_custom_"+i).style.color="rgba(255,255,255,0.2)";
		}
		_o24_enabled=false;
	}
	else {
		o24_genrand(i);
		for(var i=1;i<=25;i++){
			if(_o24_r[i]<10)
				document.getElementById("o24_custom_"+i).innerHTML="0"+_o24_r[i];
			else
				document.getElementById("o24_custom_"+i).innerHTML=+_o24_r[i];
			document.getElementById("o24_custom_"+i).style.color="#FFFFFF";
		}
		_o24_enabled=true;
	}

}

function o24_mu(i)
{
	document.getElementById("o24_custom_"+i).style.backgroundColor="rgba(255, 255, 255, 0.3)";
	if(i==100) {
		document.getElementById("o24_custom_"+i).style.color="#FFFFFF";
		return;
	}
	if(_o24_enabled) document.getElementById("o24_custom_"+i).style.color="#FFFFFF";
	else document.getElementById("o24_custom_"+i).style.color="rgba(255,255,255,0.2)";
}

function o24_mo(i)
{
	document.getElementById("o24_custom_"+i).style.backgroundColor="rgba(0,0,0,0)";
	if(i==100) {
		document.getElementById("o24_custom_"+i).style.color="#FFFFFF";
		return;
	}
	if(_o24_enabled) document.getElementById("o24_custom_"+i).style.color="#FFFFFF";
	else document.getElementById("o24_custom_"+i).style.color="rgba(255,255,255,0.2)";
}

function o24_cl(i)
{
	if(i==100) {
		document.getElementById("o24_custom_"+i).style.backgroundColor="#FFFFFF";
		document.getElementById("o24_custom_"+i).style.color="rgb(35,52,74)";
		setTimeout(function(){
			o24_mo(i);
		},200);
		o24_showrand(0);
		return;
	}
	if(_o24_r[i]!=_o24_current) return;
	if(_o24_current==1) {
		_d=new Date();
		o24_showrand(i)
	}
	document.getElementById("o24_custom_"+i).style.backgroundColor="#00FF00";
	document.getElementById("o24_custom_"+i).style.color="#FFFFFFFF";
	setTimeout(function(){
		o24_mo(i);
	},100);
	if(_o24_current==25) {
		var d=new Date();
		var delta=d.getTime()-_d.getTime();
		alert("本次： " +delta/1000+" 秒");
		_a_addi[1]++;
		if(_a[_i]==0 || _a[_i]>delta) _a[_i]=delta;
		storedata();
		document.getElementById("optionstitle").innerHTML="最短： "+_a[_i]/1000+" 秒";
	}
	_o24_current++;
}


</script>


