		document.getElementById("button").style.fontSize="2.5em";
		document.getElementById("button").style.width="8em";

	}
	if(document.documentElement.clientWidth/document.documentElement.clientHeight<1600/1059){
		var w=document.documentElement.clientHeight/1059*1600;
		document.getElementById("backgroundp").style.width="auto"
		document.getElementById("backgroundp").style.height="100%";
		document.getElementById("backgroundp").style.marginTop=0;
		document.getElementById("backgroundp").style.marginLeft=
			-(w-document.documentElement.clientWidth)/2+"px";
	}
	else {
		var h=document.documentElement.clientWidth/1600*1059;
		document.getElementById("backgroundp").style.width="100%";
		document.getElementById("backgroundp").style.height="auto";
		document.getElementById("backgroundp").style.marginLeft=0;
		document.getElementById("backgroundp").style.marginTop=
			-(h-document.documentElement.clientHeight)/2+"px";
	}
	document.getElementById("buttondiv").style.top=
		document.getElementById("header").offsetHeight+
		document.getElementById("header").offsetTop+
		"px";
	document.getElementById("buttondiv").style.height=
		document.getElementById("bottomline").offsetTop-
		document.getElementById("header").offsetHeight-
		document.getElementById("header").offsetTop+
		"px";
	document.getElementById("button").style.top=
		document.getElementById("buttondiv").offsetHeight/2-
		document.getElementById("button").offsetHeight/2+
		"px";
	document.getElementById("button").style.left=
		document.getElementById("buttondiv").offsetWidth/2-
		document.getElementById("button").offsetWidth/2+
		"px";
	
	//document.getElementById("buttondiv").style.marginBottom="8%";
    /*document.getElementById("background").style.backgroundSize = document.documentElement.clientHeight.toString() + "px " + document.documentElement.clientWidth.toString()+"px";*/
}

function divresize2()
{
	var w=document.documentElement.clientWidth;
	var h=document.documentElement.clientHeight;
	
	if(w/h<1600/1059){
		var w2=h/1059*1600;
		document.getElementById("backgroundp").style.width="auto"
		document.getElementById("backgroundp").style.height="100%";
		document.getElementById("backgroundp").style.top=0;
		document.getElementById("backgroundp").style.left= -(w2-w)/2+"px";
	}
	else {
		var h2=w/1600*1059;
		document.getElementById("backgroundp").style.width="100%";
		document.getElementById("backgroundp").style.height="auto";
		document.getElementById("backgroundp").style.left=0;
		document.getElementById("backgroundp").style.top= -(h2-h)/2+"px";
	}
	
	if(w<=_w){
		document.getElementById("maindiv").style.marginLeft="0px";
		document.getElementById("maindiv").style.width="100%";
	}
	else {
		document.getElementById("maindiv").style.width=_w+"px";
		document.getElementById("maindiv").style.marginLeft=w/2-_w/2+"px";
	}
	if(document.getElementById("buttondiv2") && document.getElementById("questiondiv")) {
		document.getElementById("buttondiv2").style.marginTop=
		document.getElementById("questiondiv").style.height=
			h-document.getElementById("buttondiv2").offsetHeight+"px";
	}
	if(document.getElementById("epilogue_t")){
		document.getElementById("epilogue_t").style.height=
			document.getElementById("epilogue").offsetHeight+"px";
		if(w<=_w){
